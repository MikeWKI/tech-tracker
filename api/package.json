{
  "name": "api",
  "version": "1.0.0",
  "description": "Tech Tracker backend",
  "main": "src/index.js",
  "type": "commonjs",
  "scripts": {
  "generate": "./node_modules/.bin/prisma generate",
  "migrate": "./node_modules/.bin/prisma migrate deploy",
  "seed": "node scripts/seed.js",
  "dev": "nodemon --watch src --exec \"npm run generate && node src/index.js\"",
  "start": "npm run generate && npm run migrate && node src/index.js",
  "build": "npm run generate && npm run migrate",
  "postinstall": "chmod +x node_modules/.bin/*"
},
  "dependencies": {
    "@prisma/client": "^6.10.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.0",
    "express": "^5.1.0",
    "pg": "^8.16.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "prisma": "^6.10.1",
    "nodemon": "^2.0.22"
  },
  "license": "MIT"
}
